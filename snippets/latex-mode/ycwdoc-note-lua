\NewDocumentCommand{\ycwdd}{O{\mathrm{d}} m m m m}{
  \frac{{#1}^{#2}\IfValueTF{#4}{#3}{}}{
    {#1}\IfValueTF{#4}{
      \IfValueTF{#5}{#4{#1}#5}{{#4}^{#2}}
    }{{#3}^{#2}}}}
