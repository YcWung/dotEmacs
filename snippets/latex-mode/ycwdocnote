\newcommand{\norm}[1]{\Vert #1 \Vert}
\newcommand{\inn}[2]{\langle #1,#2\rangle}
\newcommand{\lMod}[1]{#1\text{-}\mathrm{Mod}}
\newcommand{\rMod}[1]{\mathrm{Mod}\text{-}#1}
\newcommand{\biMod}[2]{{#1\text{-}\mathrm{Mod}\text{-}#2}}
\renewcommand{\diff}{\mathrm{d}}
\NewDocumentCommand{\dd}{O{} >{\SplitArgument{1}{|}}m}{\ycwdd{#1}#2}
\NewDocumentCommand{\ycwdd}{m m m}{
  \frac{\mathrm{d}^{#1}\IfValueTF{#3}{#2}{}}{
    \mathrm{d}\IfValueTF{#3}{#3}{#2}^{#1}}}
\NewDocumentCommand{\pp}{O{} >{\SplitArgument{1}{|}}m}{\ycwpp{#1}#2}
\NewDocumentCommand{\ycwpp}{m m m}{
  \frac{\partial^{#1}\IfValueTF{#3}{#2}{}}{
    \partial\IfValueTF{#3}{#3}{#2}^{#1}}}
\NewDocumentCommand{\tridiagm}{O{4}>{\SplitArgument{2}{|}}m}{\ycwtridiagm{#1}#2}
\NewDocumentCommand{\ycwtridiagm}{m m m m}{
  \begin{pmatrix}
    #3 & #4 &&\\\\
    #2 & #3 & \ddots &\\\\
    & \ddots & \ddots & #4\\\\
    && #2 & #3
  \end{pmatrix}}
\NewDocumentCommand{\colvec}{O{n} m}{
  \begin{pmatrix}
    #2_1 \\\\ #2_2 \\\\ \vdots \\\\#2_{#1}
  \end{pmatrix}
}
\newcommand{\nn}[1]{{\bf #1}}
